

<view class="page">
  
	
	<view class="info-body clearfix">
		<view class="imgBox">
			<image src="{{authInfo.headImage}}" class="image" />
		</view>
		<view class="contentBox" style="width:{{windowWidth-140}}px">
			<view class="clearfix item">
				<text class="p1">{{authInfo.name}}</text>
				<text class="p2">{{authInfo.juweihui}}</text>
			</view>
			<view class="clearfix item">
				<view class="imgBody">
					<image src="/static/images/face_10.png" class="img" />
				</view>
				<view class="text">{{authInfo.idNumber}}</view>
			</view>
			<view class="clearfix item">
				<view class="imgBody">
					<image src="/static/images/face_11.png" class="img" />
				</view>
				<view class="text">{{authInfo.registerTime}}</view>
			</view>
			<view class="clearfix item">
				<view class="imgBody">
					<image src="/static/images/face_12.png" class="img" />
				</view>
				<view class="text">{{authInfo.registerLocation}}</view>
			</view>
			<view class="clearfix item">
				<view class="progressTitle">认证相似度</view>
				<progress
					class="progress"
					percent="{{authInfo.score}}"
					show-info
					stroke-width="10"
					backgroundColor="#e0e9f2"
					activeColor="#fcdf39"/>
			</view>
		</view>
	</view>

   
</view>