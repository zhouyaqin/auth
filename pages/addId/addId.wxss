
.pages{
    position: relative;
    background: #f1f2f4;
}

.id_title{
  text-align: left;
  font-size: 14px;
  color: #b5bacd;
  padding: 0 10px;
  border-left: 4px solid #577baf;
  margin: 10px;
}

.identity-ct{
  position: relative;
  padding: 0 10px;
}

.content{
  width: 160px;
  height: 105px;
  margin-bottom: 20rpx;
  border:1px solid #dadada;
  padding: 10rpx;
  position: relative
  
}
.content .errorMass{
  position: absolute;
  padding: 0 5px;
  height: 115px;
  z-index: 9;
  background:rgba(0,0,0,0.5);
  top: 0;
  left: 0;
  border-radius: 10rpx;
   text-align: center;
  font-size: 26rpx;
  color: red;
  
  display: -webkit-box;
  -webkit-box-orient: horizontal;
  -webkit-box-pack: center;
  -webkit-box-align: center;

}


.content.left{
  margin-right: 36rpx;
}
.content .image{
  width: 100%;
  height: 208rpx;
  background-size: 100% 100%;
  
}

.identity-phone{
  width: 300rpx;
  margin: 0 auto;
  margin-bottom: 30rpx;
  height: 56rpx;
  margin-top: 25rpx;
  font-size: 30rpx;

}
.identity-phone image{
  width: 50rpx;
  height:50rpx;
  display: block;
  float: left
}
.identity-phone input{
  display: block;
  width: 210rpx;
  margin-left: 30rpx;
  float: left;
  border-bottom: 1px solid #b6c9e0;
 

}
.relationship{
  width:698rpx;
  height: 75rpx;
  margin: 0 auto;
  background: #eff2f7;
  margin-bottom: 20rpx;
  font-size: 30rpx;
  padding-left: 20rpx;
  border-radius: 10rpx;
  text-align: center;
}

.SubmitMass{
  background: #82b654;
  border-radius: 5px;
  color: #ffffff;
  margin: 30px 40px;
  font-size: 18px;
}

.choiceMass{
  margin-bottom: 20rpx;
  height: 72rpx;
}

.choiceMass .section{
  width: 332rpx;
  height: 72rpx;
  background: #ecf1f7;
  float: left;
  font-size: 28rpx;
  line-height: 72rpx;
  text-align: center;
  margin: 0 17rpx;
  color: #b5bace;

}
.picker{
  width: 332rpx;
  color: #b5bace;
}
.content .successimg{
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  right: 0rpx;
  bottom: 0rpx;
  right:-7rpx;
  bottom:-5rpx;
  z-index: 2;

}

.box1{
  margin: 8px 0;
  padding: 10px 0;
  background: #ffffff;
}

.phoneColor{
  color: #b5bace;
}